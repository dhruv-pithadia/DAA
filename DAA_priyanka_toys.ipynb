    "def toys(w):\n",
    "    w.sort()\n",
    "    containers = 0\n",
    "    i = 0\n",
    "    n = len(w)\n",
    "    while i < n:\n",
    "        containers += 1\n",
    "        limit = w[i] + 4\n",
    "        while i < n and w[i] <= limit:\n",
    "            i += 1\n",
    "    return containers\n",
    "\n",
    "w = [1, 2, 21, 7, 12, 14, 21]\n",
    "print(toys(w))"
   ]
  }
